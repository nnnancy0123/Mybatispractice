<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.KinMapper">


	<insert id="newUser" parameterType="String">

		insert into kin_0903(id,name)values(#{id},#{name});

	</insert>
	
<select id="selectById" parameterType="String" resultType="com.example.demo.bean.InfoBean">

  SELECT * FROM kin_0903 WHERE ID = #{id}
  
</select>

</mapper>
